{
  "name": "parking project",
  "version": "0.0.0",
  "scripts": {
    "build": "npx ts-node ./scripts/build.ts",
    "dev": "NODE_ENV=development npx ts-node \"./src\"",
    "win:dev": "cross-env NODE_ENV=development npx ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src -e ts",
    "lint": "npx eslint ./src",
    "lint:tests": "npx eslint ./spec",
    "start": "NODE_ENV=production node -r ./preload.js ./dist",
    "test": "NODE_ENV=test npx ts-node ./spec",
    "test:hot": "nodemon --exec \"npm run test\" --watch ./src --watch ./spec -e ts",
    "test:e2e": "jest --testPathPattern=spec/e2e"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/mongoose": "^5.11.96",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.34.1",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-generator-typescript": "^2.4.25",
    "helmet": "^8.0.0",
    "inserturlparams": "^2.0.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.0.9",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "mongoose": "^8.7.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@stylistic/eslint-plugin-ts": "^2.9.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cypress": "^0.1.6",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jasmine": "^5.1.4",
    "@types/jsonfile": "^6.1.4",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.7.5",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.2",
    "drizzle-kit": "^0.25.0",
    "eslint": "^9.12.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-n": "^17.10.3",
    "find": "^0.3.0",
    "fs-extra": "^11.2.0",
    "jasmine": "^5.3.1",
    "nodemon": "^3.1.7",
    "pg": "^8.13.1",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.1"
  }
}
